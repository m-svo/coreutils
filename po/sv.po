"  -b, --number-nonblank    numrera icke-tomma rader\n"
"  -e                       som -vE\n"
"  -E, --show-ends          visa $ i slutet av varje rad\n"
"  -n, --number             numrera alla rader\n"
"  -s, --squeeze-blank      aldrig mer än en ensam tom rad\n"

