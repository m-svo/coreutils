"  %k   hour ( 0..23)\n"
"  %l   hour ( 1..12)\n"
"  %m   month (01..12)\n"
"  %M   minute (00..59)\n"
msgstr ""
"  %k   Stunde ( 0..23)\n"
"  %l   Stunde ( 1..12)\n"
"  %m   Monat (01..12)\n"
"  %M   Minute (00..59)\n"

#: src/date.c:180
msgid ""
"  %n   a newline\n"
